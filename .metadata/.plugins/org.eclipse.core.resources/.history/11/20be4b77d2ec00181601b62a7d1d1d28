import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Toy woody =new Toy("Woody","Tom","Hanks");
		Toy buzz =new Toy("Buzz Lightyear","Tim","Allen");
		Toy slinky =new Toy("Slinky Dog","Jim","Varney");
		Toy ham =new Toy("Hamm","John","Ratzenberger");
		Toy rex =new Toy("Rex","Wallace","Shawn");
		Toy potato =new Toy("Mr. Potato Head","Don","Rickles");
		
		
		ArrayList<Toy> toys = new ArrayList<Toy>();
		toys.add(woody);
		toys.add(buzz);
		toys.add(slinky);
		toys.add(ham);
		toys.add(rex);
		toys.add(potato);
		
		for(Toy toy:toys) {
			String output=(toy.getCharName()+" "+toy.getfName()+" "+toy.getlName());
			System.out.println(output);
		}
	}

}
